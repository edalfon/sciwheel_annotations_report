{
  "name": "Annotations Report for Sciwheel",
  "version": "0.1",
  "description": "Generates a visualization for the annotations from an opened reference in Sciwheel",
  "permissions": ["storage", "webNavigation", "declarativeContent", "tabs", "activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "page_action": {
    "default_icon": {
      "16": "images/sciwheel_annotations_report_icon.png",
      "32": "images/sciwheel_annotations_report_icon.png",
      "48": "images/sciwheel_annotations_report_icon.png",
      "128": "images/sciwheel_annotations_report_icon.png"
    }
  },
  "manifest_version": 2
}